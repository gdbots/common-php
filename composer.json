{
  "name": "gdbots/common",
  "description": "Common classes for php projects.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "ext-bcmath": "*",
    "moontoast/math": "~1.1",
    "ramsey/uuid": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
