{
  "name": "gdbots/common",
  "description": "Common classes for php projects.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.4",
    "ext-bcmath": "*",
    "moontoast/math": "~1.1",
    "rhumsaa/uuid": "~2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\": "tests"
    }
  },
  "replace": {
    "gdbots/common-php": "self.version"
  }
}
